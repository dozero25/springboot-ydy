<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.springbootydy.repository.OptionRepository">
    <insert id="saveCategory" parameterType="com.study.springbootydy.web.dto.CategoryDto"
    useGeneratedKeys="true" keyProperty="categoryId">
        insert into category_mst
        values(0, #{categoryName})
    </insert>
</mapper>
